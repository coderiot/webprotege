<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
     <ui:style>
        .thread-style {
            /*line-height: 12px;*/
        }
        .button-bar-style {
            padding: 2px;
        }
    </ui:style>
    <g:HTMLPanel height="100%" width="100%" addStyleNames="{style.thread-style}">
        <g:HTMLPanel addStyleNames="{style.button-bar-style}">
            <g:Button ui:field='commit' addStyleNames="button-style"> Commit changes to DBpedia </g:Button>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>